type Profile {
  id: ID!
  fullName: String!
  username: String!
  profileUrl: String
  createdAt: String!
  updatedAt: String!
}

type Verdict {
  analysis: String!
  score: Int!
  suitability: String!
}

type VerdictInput {
  username: String!
  product: String!
}

type MyVerdictInput {
  product: String!
}

type Query {
  getProfiles: [Profile!]!
  getVerdict(VerdictInput!): Verdict!
  getMyVerdict(MyVerdictInput!): Verdict!
}
